{
  "name": "@kevinoconnell/mcpkit",
  "version": "1.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Easy setup for MCPs with Browserbase - run browser automation anywhere with just an API key",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcpkit": "dist/cli.js",
    "mcpkit-server": "dist/mcp/server.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli.ts",
    "dev:server": "tsx src/mcp/server.ts",
    "build": "tsc",
    "start": "node dist/cli.js",
    "start:server": "node dist/mcp/server.js",
    "generate": "tsx src/cli.ts",
    "secrets:setup": "tsx src/secrets/cli.ts"
  },
  "keywords": [
    "mcp",
    "stagehand",
    "browser-automation",
    "ai",
    "model-context-protocol"
  ],
  "author": "Kevin O'Connell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinoconnell/mcpkit.git"
  },
  "bugs": {
    "url": "https://github.com/kevinoconnell/mcpkit/issues"
  },
  "homepage": "https://github.com/kevinoconnell/mcpkit#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@browserbasehq/stagehand": "^3.0.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.9",
    "ai": "^5.0.86",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "dotenv": "^16.0.0",
    "prompts": "^2.4.2",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
